rule("Trigger : Only Headshots")
{
	event
	{
		Player Took Damage;
		All;
		All;
	}

	conditions
	{
		Event Player.Active_Effects_ID[22] != 0;
		Event Was Critical Hit != True;
		Victim != Attacker;
	}

	actions
	{
		Set Player Health(Event Player, Health(Event Player) + Event Damage);
	}
}

