rule("Trigger : Thorns")
{
	event
	{
		Player Took Damage;
		All;
		All;
	}

	conditions
	{
		Event Player.Active_Effects_ID[30] != 0;
		Victim != Attacker;
		"To Prevent Feedback Loop When They Both Have Thorns"
		Attacker.Active_Effects_ID[30] == Null;
	}

	actions
	{
		Damage(Attacker, Event Player, Event Damage / 2);
	}
}

