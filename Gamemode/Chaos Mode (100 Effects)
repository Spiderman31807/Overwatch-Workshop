rule("Package : Kiriko - Codename K-2000 Blademaster")
{
	event
	{
		Subroutine;
		Package_K2000_Blademaster;
	}

	actions
	{
		Small Message(Event Player, Event Player.Hide_Effects ? Custom String("???? ??????? ?????? ??? ???") : Custom String(
