		Array Contains(Event Player.Heal_Friendlys, Button(Primary Fire)) != True;
		Team Of(Event Player) == Team Of(First Of(Event Player.Target_Order));
		Global.Use_Complex_Abilitys == True;
	}

	actions
	{
