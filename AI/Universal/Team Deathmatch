rule("Nodes Setup")
{
	event
	{
		Subroutine;
		Node_Setup;
	}

	actions
	{
		If(Current Map == Map(Black Forest) || Current Map == Map(Black Forest Winter));
			Call Subroutine(Black_Forest);
		End;
		If(Current Map == Map(Castillo));
			Call Subroutine(Castillo);
		End;
		If(Current Map == Map(Ecopoint: Antarctica) || Current Map == Map(Ecopoint: Antarctica Winter));
			Call Subroutine(Ecopoint_Antarctica);
		End;
		If(Current Map == Map(Château Guillard) || Current Map == Map(Château Guillard Halloween));
			Call Subroutine(Chateau_Guillard);
		End;
		If(Current Map == Map(Malevento));
			Call Subroutine(Malevento);
		End;
		If(Current Map == Map(Petra));
			Call Subroutine(Petra);
		End;
		If(Current Map == Map(Kanezaka));
			Call Subroutine(Kanezaka);
		End;
		If(Current Map == Map(Route 66));
			Call Subroutine(Route66);
		End;
		If(Current Map == Map(Havana));
			Call Subroutine(Havana);
		End;
		If(Current Map == Map(Dorado));
			Call Subroutine(Dorado);
		End;
		If(Current Map == Map(Hollywood) || Current Map == Map(Hollywood Halloween));
			Call Subroutine(Hollywood);
		End;
		If(Current Map == Map(Blizzard World Winter) || Current Map == Map(Blizzard World Winter));
			Call Subroutine(Blizz_World);
		End;
		If(Current Map == Map(Eichenwalde) || Current Map == Map(Eichenwalde Halloween));
			Call Subroutine(Eichenwalde);
		End;
		If(Current Map == Map(King's Row) || Current Map == Map(King's Row Winter));
			Call Subroutine(Kings_Row);
		End;
		For Global Variable(Jump_Index, 0, Count Of(Global.Jump_End), 1);
			Create Projectile(Orb Projectile, Null, Global.Jump_End[Global.Jump_Index], Down, To World, Damage, All Teams, 0, 1, 8,
				Bad Explosion, Explosion Sound, 0, 50, 1, 0, 0, 0);
		End;
	}
}

