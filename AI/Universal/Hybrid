rule("Global Setup")
{
	event
	{
		Subroutine;
		Global_Setup;
	}

	actions
	{
		Global.Vaild_Heros = Array(Custom String("Reinhardt"), Custom String("Winston"), Custom String("Roadhog"), Custom String("Zarya"),
			Custom String("D.Va"), Custom String("Orisa"), Custom String("Wrecking Ball"), Custom String("Sigma"), Custom String("Mercy"),
			Custom String("Zenyatta"), Custom String("LÃºcio"), Custom String("Ana"), Custom String("Brigitte"), Custom String("Moira"),
			Custom String("Baptiste"), Custom String("Reaper"), Custom String("Tracer"), Custom String("Hanzo"), Custom String("TorbjÃ¶rn"),
